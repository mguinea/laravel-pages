{
    "name": "mguinea/laravel-pages",
    "description": "Laravel CMS",
    "keywords": [
        "cms",
        "laravel"
    ],
    "homepage": "https://github.com/mguinea/laravel-pages",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Guinea",
            "email": "develop.marcguinea@gmail.com"
        }
    ],
    "require": {
        "php" : ">=8.1",
        "illuminate/database": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mguinea\\LaravelPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mguinea\\LaravelPages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mguinea\\Pages\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
